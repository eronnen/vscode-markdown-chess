{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off"
  },
  "ignorePatterns": ["out/*", "dist/*", "**/*.d.ts"],
  "env": {
    "commonjs": true
  }
}
